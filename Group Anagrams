class Solution {
public:
    vector<vector<string>> groupAnagrams(vector<string>& strs) {
        unordered_map<string,vector<string>>mpp;
        for(auto &s:strs){
            string key=s;
            sort(key.begin(),key.end());
            mpp[key].push_back(s);
        }
        vector<vector<string>>sol;
        for(auto &it:mpp){
            sol.push_back(it.second);
        }
        return sol;
    }
};
